{
  "name": "@icalialabs/cc-test-reporter-base",
  "version": "0.0.1",
  "description": "Base commands to run the Code Climate Test Reporter on Github Actions",
  "main": "lib/main.js",
  "scripts": {
    "debug-pre": "yarn run build && node -r dotenv/config pre.js",
    "debug-main": "yarn run build && node -r dotenv/config main.js",
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest --coverage",
    "pre-checkin": "yarn run format && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icalia-actions/cc-test-reporter-base.git"
  },
  "keywords": [
    "actions",
    "icalia",
    "aws",
    "ecs",
    "build",
    "push"
  ],
  "author": "Icalia Labs",
  "contributors": [
    {
      "name": "Vov",
      "url": "https://ical.ink/vov"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.2.7",
    "@actions/exec": "^1.0.4",
    "@actions/tool-cache": "^1.6.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@vercel/ncc": "^0.28.3",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "jest-runtime": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "typescript-formatter": "^7.2.2",
    "yaml": "^1.10.2"
  }
}
